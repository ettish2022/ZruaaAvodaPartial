                <elementReference>$Record.ConvertedAccountId</elementReference>
            </value>
        </inputAssignments>
        <inputAssignments>
            <field>NumberRequested__c</field>
            <value>
                <elementReference>$Record.No_of_Employees_Requested__c</elementReference>
            </value>
        </inputAssignments>
        <inputAssignments>
            <field>Program__c</field>
            <value>
                <elementReference>Var_Program_Id</elementReference>
            </value>
        </inputAssignments>
        <inputAssignments>
            <field>Responsible_contact_Email__c</field>
            <value>
                <elementReference>Get_Program.Responsible_Contact__r.Email</elementReference>
            </value>
        </inputAssignments>
        <object>Case</object>
